{
    "workbench.colorTheme": "Default High Contrast",
    "workbench.iconTheme": "vscode-icons",
    "git.autofetch": true,
    "security.workspace.trust.untrustedFiles": "open",
    "al.enableCodeActions": true,
    "errorLens.messageTemplate": "$message [$code]",
    "alOutline.completionProviders": [
        "VariableDataTypes",
        "VariableNames",
        "VariableNamesWithType"
    ],
    "editor.stickyScroll.enabled": true,
    "vscode-pets.petType": "cat",
    "vscode-pets.petSize": "medium",
    "vscode-pets.petColor": "black",
    "vscode-pets.throwBallWithMouse": true,
    "git.openRepositoryInParentFolders": "always",
    "breadcrumbs.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "errorLens.excludeBySource": [
        "AL(AL0573)"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.env.windows": {},
    "al.inlayhints.functionReturnTypes.enabled": true,
    "al.inlayhints.parameterNames.enabled": true,
    "editor.inlayHints.enabled": "on",
    "editor.fontFamily": "",
    "editor.inlayHints.fontSize": 3,
    "editor.inlayHints.padding": false,
    "window.zoomLevel": 1,
    "alTranslationCenter.general.customLanguageMapping": {
        "ESP": "es-ES_tradnl|es-ES"
    },
    "AlProjectConfigurator.PTETemplate": "https://github.com/NovoaDev/ExamplePTECFG.git",
    "AlProjectConfigurator.ProductTemplate": "https://github.com/NovoaDev/ExampleAppSourceCFG.git",
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4o",
    "github.copilot.chat.codeGeneration.instructions": [
        {
            "text": "When you answer me always do it in Spanish using a childish and friendly tone. Start with a title with some emote that says !Holi holi followed by an affectionate and very funny nickname adding at the end a de and some name of a food. Generate a different one each time, for example: ¡Holi holi Pastelito de atún! o  ¡Holi holi Solecito de Bizcocho!."
        },
        {
            "text": "when you generate code don't explain it to me."
        },
        {
            "text": "All code must be written in English."
        },
        {
            "text": "Avoid adding redundant or unhelpful comments."
        }
    ]
}